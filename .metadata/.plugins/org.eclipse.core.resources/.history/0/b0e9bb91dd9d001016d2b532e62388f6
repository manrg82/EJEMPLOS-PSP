package pspt1;

public class LanzadorMultiplicador {
	public void lanzarMultiplicador(int n1,int n2,String fic) {
		ProcessBuilder pb;
        try {
                pb = new ProcessBuilder("java","\"C:\\EJEMPLOS-PSP\\pspt1\\bin\\pspt1\\Multiplicador.class\"" );
                pb.start();
        } catch (Exception e) {
                e.printStackTrace();
        }
	}
}
