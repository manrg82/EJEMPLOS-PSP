package pspt1;

public class ContaVocal {
	public int contVocal(String fl, char voc) {
		int resul=0;
		try{
			for(int i=0;i<fl.length();i++) {
				if(fl.charAt(i)==voc) {
					resul++;
				}
			}
		}catch(Exception e){
			e.printStackTrace();
		}
		return resul;
	}
	public static void main(String[] Args) {
		LanzarProcesos lz=new LanzarProcesos();
		lz.lanzarContador("files/texto.txt", 'a');
	}
}
