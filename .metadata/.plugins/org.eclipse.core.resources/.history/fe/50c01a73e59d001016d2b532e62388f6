package pspt1;

public class LanzadorMultiplicador {
	public void lanzarMultiplicador(int n1,int n2,String fic) {
		ProcessBuilder pb;
        try {
        	String clase = "com.pspt1.Multiplicador";
        	String classPath = ".;./bin";
			
     	   pb = new ProcessBuilder("java", "-cp", classPath, clase,n1,n2);
                pb.start();
        } catch (Exception e) {
                e.printStackTrace();
        }
	}
}
